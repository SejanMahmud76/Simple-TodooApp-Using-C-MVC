@model TodooApp.Models.Todo


<h1>Create a new Todo</h1>

<form  asp-action="Create" method="post">

    <divv>


        <label>Title:</label>
        <input asp-for ="Title" />

    </divv>



    <divv>
        <label>Is Done:</label>
        <input asp-for="IsDone" type="checkbox" />
    </divv>


    <button type="submit">Add Todo</button>







</form>