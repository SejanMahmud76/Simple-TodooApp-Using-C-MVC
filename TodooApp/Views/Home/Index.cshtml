


@model List<TodooApp.Models.Todo>

    <h1> My Todo List</h1>

<p>
    <a asp-action="Create">Add New Todo</a>
</p>

    <table bored="1">

        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Is Done? </th>
        </tr>


        @foreach(var todo in Model)
    {
        

        <tr>
            
            <td>@todo.Id</td>
            <td>@todo.Title</td>
            <td>@todo.IsDone</td>



        </tr>
    }

    </table>


